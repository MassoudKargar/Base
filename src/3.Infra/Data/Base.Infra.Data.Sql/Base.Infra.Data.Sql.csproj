<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Nullable>enable</Nullable>
		<Version>8.0.0</Version>
		<Authors>Masoud kargar</Authors>
		<ImplicitUsings>enable</ImplicitUsings>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<RepositoryUrl>https://github.com/MassoudKargar/Base</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageIcon>base.png</PackageIcon>
	</PropertyGroup>
	<ItemGroup>
		<None Include="README.md" Pack="true" PackagePath="" />
		<None Include="base.png" Pack="true" PackagePath="" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="8.0.4" />
		<PackageReference Include="Base.Extensions.ChangeDataLog.Abstractions" Version="8.0.0" />
		<PackageReference Include="Base.Extensions.UsersManagement.Abstractions" Version="8.0.0" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\..\..\2.Core\Base.Core.Contracts\Base.Core.Contracts.csproj" />
	  <ProjectReference Include="..\..\..\2.Core\Base.Core.Domain.Toolkits\Base.Core.Domains.Toolkits.csproj" />
	  <ProjectReference Include="..\..\..\2.Core\Base.Core.Domain\Base.Core.Domains.csproj" />
	</ItemGroup>

</Project>
